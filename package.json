{
  "name": "expressjs-api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs -d src/database/datasource.ts",
    "migrate": "typeorm-ts-node-commonjs -d src/database/datasource.ts migration:generate -p",
    "start": "node dist/index.js",
    "lint": "echo \"No linter yet\""
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.6",
    "debug": "~2.6.9",
    "dotenv": "^17.3.1",
    "express": "~4.16.1",
    "helmet": "^8.1.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "~1.9.1",
    "oracledb": "^6.10.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}